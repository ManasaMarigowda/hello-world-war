pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
              sh "rm -rf hello-world-war "
                sh "git clone https://github.com/ManasaMarigowda/hello-world-war"
            }
  }
        stage('build') {
            steps {
      sh "sudo apt update"
                sh "mvn clean package"
            }
       
        }
    }
}
